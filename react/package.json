        "react-modal": "^3.14.4",
        "retry": "^0.13.1",
        "svgo": "^3.0.3",
        "what-input": "^5.2.10",
        "xss": "^1.0.10"
    },
    "overrides": {
        "trim": ">=0.0.3",
        "trim-newlines": "^3.0.1",
        "glob-parent": "^6.0.1",
        "semver": "^7.5.3",
        "optionator": "0.9.3"
    }
}
